{
  "name": "u-eats",
  "version": "0.1.5",
  "author": "Dawid Zajac <zajdawid@protonmail.com>",
  "private": true,
  "homepage": "/u-eats/",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.3",
    "@mui/lab": "^5.0.0-alpha.85",
    "@mui/material": "^5.8.3",
    "@popperjs/core": "^2.11.5",
    "@reduxjs/toolkit": "^1.8.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "axios": "^0.24.0",
    "date-fns": "^2.28.0",
    "decimal.js-light": "^2.5.1",
    "downshift": "^6.1.7",
    "faker": "^5.5.3",
    "framer-motion": "^6.3.11",
    "i18next": "^21.6.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "lodash-es": "^4.17.21",
    "msw": "^0.42.1",
    "polished": "^4.1.3",
    "query-string": "^7.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet-async": "^1.2.2",
    "react-hook-form": "^7.31.3",
    "react-hot-toast": "^2.2.0",
    "react-i18next": "^11.16.2",
    "react-loading-skeleton": "^3.1.0",
    "react-popper": "^2.3.0",
    "react-query": "^3.39.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "react-table": "^7.8.0",
    "styled-components": "^5.3.3",
    "usehooks-ts": "^2.5.4"
  },
  "scripts": {
    "start": "npm run start:development",
    "start:development": "cross-env REACT_APP_CONFIG=DEVELOPMENT react-scripts start",
    "start:production": "cross-env REACT_APP_CONFIG=PRODUCTION react-scripts start",
    "build": "npm run build:production",
    "build:development": "cross-env REACT_APP_CONFIG=DEVELOPMENT react-scripts build",
    "build:production": "cross-env REACT_APP_CONFIG=PRODUCTION react-scripts build && cp build/index.html build/404.html",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --no-error-on-unmatched-pattern --fix src/**/*.{js,jsx,ts,tsx}",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "npm run lint:fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.5",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-table": "^7.7.12",
    "@types/styled-components": "^5.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "gh-pages": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "typescript": "^4.5.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
